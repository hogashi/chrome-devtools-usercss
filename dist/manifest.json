{
  "manifest_version": 3,
  "name": "chrome-devtools-usercss",
  "version": "0.1.0",
  "description": "usercss for Chrome DevTools",
  "author": "hogashi",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": ["storage"],
  "devtools_page": "devtools.html",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "chrome-devtools-usercss",
    "default_popup": "popup.html"
  },
  "options_page": "popup.html"
}
